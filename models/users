var mongoose=require('mongoose');
var Schema=mongoose.Schema;
var userschema=new Schema({
   firstname:String,
    lastname:String,
    age:String,
    gender:String,
    address:String,
    joining_date:String,
    password:String,
    /*book_id:String,
    book_name:String,
    author_name:String,
    book_issued:String,
    book_return:String,
    password:String*/
});
var User=mongoose.model('User',userschema);
module.exports=User;
